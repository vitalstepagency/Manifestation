# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# ⚠️ ADMIN ONLY - SERVICE ROLE KEY (NEVER COMMIT THIS!)
# This key bypasses ALL security rules - use with extreme caution
# Only needed for admin functions
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Development
VITE_APP_ENV=development

# Unsplash API (Optional - for search-relevant images)
# Get your free API key at: https://unsplash.com/oauth/applications
# Without this key, app uses Lorem Picsum (random but beautiful photos)
VITE_UNSPLASH_ACCESS_KEY=your_unsplash_access_key